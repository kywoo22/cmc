	<script>
	document.addEventListener('DOMContentLoaded', function() {
	  const topicBoxes = document.querySelectorAll('.topic-box');
	  topicBoxes.forEach(topicBox => {
	    const leftArrow = topicBox.querySelector('.left-arrow');
	    const rightArrow = topicBox.querySelector('.right-arrow');
	    const slides = topicBox.querySelectorAll('.topic-slide');
	    let currentIndex = 0;
	    const totalSlides = slides.length;
	    // 화살표 초기 상태 (투명 + 클릭 불가 + 부드러운 트랜지션)
	    [leftArrow, rightArrow].forEach(arrow => {
	      arrow.style.opacity = '0';
	      arrow.style.pointerEvents = 'none';
	      arrow.style.transition = 'opacity 0.3s ease'; 
	      // 필요에 따라 duration, easing 조절
	    });
	    // 슬라이드 이동
	    function updateSlider() {
	      slides.forEach((slide, idx) => {
	        slide.style.transform = `translateX(-${currentIndex * 100}%)`;
	      });
	      updateArrows();
	    }
	    // 화살표 상태 업데이트
	    function updateArrows() {
	      // 호버 중이 아니면 둘 다 숨기기
	      if (!topicBox.matches(':hover')) {
	        hideArrow(leftArrow);
	        hideArrow(rightArrow);
	        return;
	      }
	      // 호버 중이라면 currentIndex에 맞춰 보이거나 숨기기
	      if (currentIndex === 0) {
	        hideArrow(leftArrow);
	      } else {
	        showArrow(leftArrow);
	      }
	      if (currentIndex === totalSlides - 1) {
	        hideArrow(rightArrow);
	      } else {
	        showArrow(rightArrow);
	      }
	    }
	    // 화살표 숨기기
	    function hideArrow(arrow) {
	      arrow.style.opacity = '0';
	      arrow.style.pointerEvents = 'none';
	    }
	    // 화살표 보이기
	    function showArrow(arrow) {
	      arrow.style.opacity = '1';
	      arrow.style.pointerEvents = 'auto';
	    }
	    // 왼쪽 화살표 클릭
	    leftArrow.addEventListener('click', () => {
	      if (currentIndex > 0) {
	        currentIndex--;
	        updateSlider();
	      }
	    });
	    // 오른쪽 화살표 클릭
	    rightArrow.addEventListener('click', () => {
	      if (currentIndex < totalSlides - 1) {
	        currentIndex++;
	        updateSlider();
	      }
	    });
	    // 마우스 호버 이벤트
	    topicBox.addEventListener('mouseenter', () => {
	      updateArrows(); // 호버 시 화살표 상태 갱신
	    });
	    topicBox.addEventListener('mouseleave', () => {
	      // 호버 해제 시 즉시 숨기기 (fade out 애니메이션)
	      hideArrow(leftArrow);
	      hideArrow(rightArrow);
	    });
	    // 초기 설정
	    updateSlider();
	  });
	});
</script>
